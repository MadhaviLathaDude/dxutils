{
  "name": "@siddharatha/dxutils",
  "description": "Additional utilities to support vscode utilities",
  "version": "0.0.10",
  "author": "Sid @siddharatha",
  "bugs": "https://github.com/siddharatha/dxutils/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/errors": "1",
    "@salesforce/command": "^1.0.1",
    "@salesforce/core": "^1.0.1",
    "bluebird": "^3.5.3",
    "moment": "^2.24.0",
    "tslib": "1",
    "vscode": "^1.1.36"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.1.4",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "sinon": "5",
    "ts-node": "^7",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-config-standard": "^8.0.0",
    "typescript": "^3.0",
    "vscode": "^1.1.36"
  },
  "engines": {
    "node": ">=8.0.0",
    "vscode": "^1.22.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/siddharatha/dxutils",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "pull": {
        "description": "Perform a pull."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "siddharatha/dxutils",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepare": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "node node_modules/vscode/bin/install"
  },
  "contributes": {
    "configuration": {
      "title": "DxUtils Configuration",
      "properties": {
        "dxUtils.ignoreTypes": {
          "type": "array",
          "default": [],
          "description": "Specifies the metadata types that are ignored by default"
        }
      }
    }
  }
}
